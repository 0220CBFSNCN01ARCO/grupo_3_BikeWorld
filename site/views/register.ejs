  <title>Registrate en BikeWorld</title>
</head>
<body>
  <div class="container">
    <div class="row">
      <!--  Barra de navegación  -->
      <header class="main-header col-12">
      </nav>
      <nav class="nav navbar navbar-expand-lg navbar-light bg-light">
        <span class="navbar-brand mb-0 h1"><a class="navbar-brand" href="#">
          <img src="../public/images/logo.png" width="30" height="30" class="d-inline-block align-top" alt="logotipo">   BikeWorld</a>
        </span>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#menuNavegacion" aria-controls="menuNavegacion" aria-expanded="false" >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="menuNavegacion">
          <div class="row">
            <ul class="barrasuperior navbar-nav">
              <li class="nav-item"><a class="nav-link" href="#"><button type="button col-lg-2" class="btn btn-light btn-lg">Inicio</button></a></li>
              <li class="nav-item"><a class="nav-link" href="#"><button type="button col-lg-2" class="btn btn-light btn-lg">Productos</button></a></li>
              <li class="nav-item"><a class="nav-link" href="#"><button type="button col-lg-2" class="btn btn-light btn-lg">Carrito</button></a></li>
              <li class="nav-item"><a class="nav-link" href="#"><button type="button col-lg-2"class="btn btn-light btn-lg">Registrarse</button></a></li>
              <li class="nav-item"><a class="nav-link" href="#"><button type="button col-lg-2"class="btn btn-light btn-lg">Contacto</button></a></li>
            </ul>
          </div>
        </div>
      </nav>
    </header>
    <!-- Fin de la barra de navegación -->
  </div>
  <br>
  <h1 class="Titulo-pagina">Completá tus datos</h1>
  
  <!-- Formulario  -->
  <form method="post" action="">
    <div class="container">
      <div class="row">
        
        <!-- Campo nombre -->
        <div class="form-group" class="col-sm-12 col-md-6 col-lg4">
          <label for="nombre"></label>
          <input type="text" name="nombre" class="form-control" placeholder="Nombre" id="nombre" aria-describedby="emailHelp" required>
        </div>
        
        <!-- Campo Apellido -->
        <div class="form-group" class="col-sm-12 col-md-6 col-lg4">
          <label for="apellido"></label>
          <input type="text" name="apellido" class="form-control" placeholder="Apellido" id="apellido" aria-describedby="emailHelp" required>
        </div>
        
        <!-- Campo email -->
        <div class="form-group" class="col-sm-12 col-md-6 col-lg4">
          <label for="email"></label>
          <input type="email" name="email" class="form-control" placeholder="E-mail" id="email" aria-describedby="emailHelp" required>
        </div>
        
        <!-- Campo password -->
        <div class="form-group" class="col-sm-12 col-md-6 col-lg4">
          <label for="password"></label>
          <input type="password" name="password" class="form-control" placeholder="Contraseña" id="password" required>
          <small id="passwordHelpBlock" class="form-text text-muted"> La contraseña debe tener 6 caracteres como mínimo.</small>
        </div>
        
        <!-- Campo repetir password -->
        <div class="form-group" class="col-sm-12 col-md-6 col-lg4">
          <label for="repetirPassword"></label>
          <input type="password" name="passwordRepeat" class="form-control" placeholder="Repetir contraseña" id="repetirPassword" required>
        </div>
        <br>
        
        <!-- Campo botón crear cuenta -->
        <div class="col-sm-12 col-md-6 col-lg4">
          <button type="submit" class="btn btn-primary">Crear Cuenta</button>
        </div>
        
        <!-- Campo ya tenes cuenta? Ingresar -->
        <div class="col-12" class="yaTenesCuenta"> ¿Ya tenes cuenta?<a target="" href="" >  Ingresar</a>
        </div>
      </div>
    </div>
  </form>
</div>

